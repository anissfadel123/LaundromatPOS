<UserControl x:Class="LaundroDesktopUI.Components.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LaundroDesktopUI.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <PasswordBox x:Name="passwordBox"  PasswordChanged="PasswordBox_PasswordChanged" VerticalContentAlignment="Center" Padding="68 0 0 0"  
                             FontSize="20" FontWeight="Medium" Foreground="#707070" />
</UserControl>
